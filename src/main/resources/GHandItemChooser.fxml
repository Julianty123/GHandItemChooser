<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="285.0" prefWidth="361.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GHandItemChooser">
    <Button fx:id="buttonIntercept" layoutX="26.0" layoutY="162.0" mnemonicParsing="false" prefHeight="92.0" prefWidth="116.0" text="OFF!" />
    <ChoiceBox fx:id="choiceBoxItems" layoutX="181.0" layoutY="189.0" prefWidth="150.0" />
    <TextField fx:id="txtFurniId" alignment="CENTER" editable="false" layoutX="25.0" layoutY="106.0" prefHeight="31.0" prefWidth="118.0" promptText="Double click" />
    <CheckBox fx:id="checkFurniId" layoutX="159.0" layoutY="111.0" mnemonicParsing="false" text="Get Furni Id" />
    <Label layoutX="201.0" layoutY="153.0" text="Delay:" />
    <TextField fx:id="txtDelay" alignment="CENTER" layoutX="256.0" layoutY="148.0" prefHeight="31.0" prefWidth="65.0" text="1450" />
    <RadioButton fx:id="radioBlackHole" layoutX="44.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleRadioButtonChanged" text="Black Hole">
        <toggleGroup>
            <ToggleGroup fx:id="f" />
        </toggleGroup>
    </RadioButton>
    <RadioButton fx:id="radioFlowers" layoutX="44.0" layoutY="60.0" mnemonicParsing="false" onAction="#handleRadioButtonChanged" text="French Flowers" toggleGroup="$f" />
    <RadioButton fx:id="radioNormalFridge" layoutX="174.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleRadioButtonChanged" text="Normal Fridge" toggleGroup="$f" />
    <RadioButton fx:id="radioFreezeFridge" layoutX="189.0" layoutY="60.0" mnemonicParsing="false" onAction="#handleRadioButtonChanged" text="Freeze Fridge" toggleGroup="$f" />
    <CheckBox fx:id="checkShowItemId" layoutX="168.0" layoutY="239.0" mnemonicParsing="false" text="Show current itemid" />
</AnchorPane>
